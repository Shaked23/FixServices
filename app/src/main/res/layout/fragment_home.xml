<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/homeFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backg"
    tools:context=".fragments.HomeFragment"
    tools:layout_editor_absoluteX="213dp"
    tools:layout_editor_absoluteY="5dp" >

    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="match_parent">

        <TextView
            android:background="@drawable/blue"
            android:gravity="center"
            android:id="@+id/textHomeAct_FixForYou"
            android:layout_alignParentEnd="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_height="60dp"
            android:layout_marginBottom="12dp"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_width="377dp"
            android:text="Fix For You"
            android:textColor="#081B85"
            android:textSize="34sp"
            android:textStyle="bold" />

        <GridLayout
            android:columnCount="2"
            android:layout_alignParentBottom="false"
            android:layout_alignParentEnd="false"
            android:layout_alignParentStart="false"
            android:layout_below="@id/textHomeAct_FixForYou"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:rowCount="3"
            app:layout_constraintTop_toBottomOf="@id/textHomeAct_FixForYou">

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"
                android:id="@+id/cardPrices"
                android:layout_column="0"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView19"
                        android:layout_width="match_parent"
                        android:layout_height="85dp"
                        app:srcCompat="@drawable/price" />

                    <TextView
                        android:id="@+id/textViewPrices"
                        android:layout_width="103dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Prices"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"
                android:id="@+id/cardSearch"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="0"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/im"
                        android:layout_height="72dp"
                        android:layout_width="70dp"

                        app:srcCompat="@drawable/search1" />

                    <TextView
                        android:id="@+id/text_search"
                        android:layout_width="106dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Search Professional"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"
                android:id="@+id/cardDetails"
                android:layout_column="0"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imag"
                        android:layout_width="112dp"
                        android:layout_height="83dp"
                        android:src="@drawable/details1" />

                    <TextView
                        android:id="@+id/textDetails"
                        android:layout_width="100dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Your Details"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"
                android:id="@+id/cardRequests"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="1"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView1"
                        android:layout_width="112dp"
                        android:layout_height="86dp"
                        app:srcCompat="@drawable/set" />
                    " />

                    <TextView
                        android:id="@+id/textViewMeet"
                        android:layout_width="101dp"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Your Open
                     Requests"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"
                android:id="@+id/cardNewRequest"
                android:layout_column="0"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageVi"
                        android:layout_width="88dp"
                        android:layout_height="77dp"

                        app:srcCompat="@drawable/f" />

                    <TextView
                        android:id="@+id/textViewNew"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="New Request"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:foregroundGravity="fill"


                android:id="@+id/cardLogout"
                android:layout_column="1"
                android:layout_columnWeight="1"
                android:layout_height="wrap_content"
                android:layout_row="2"
                android:layout_rowWeight="1"
                android:layout_width="wrap_content"
                android:visibility="visible"
                app:cardBackgroundColor="@color/black"
                app:cardCornerRadius="50dp"
                app:cardElevation="10dp"
                app:cardUseCompatPadding="true">

                <LinearLayout
                    android:gravity="center"
                    android:layout_gravity="center_vertical|center_horizontal"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/imageView191"
                        android:layout_height="67dp"
                        android:layout_width="86dp"
                        android:src="@drawable/logout" />

                    <TextView
                        android:id="@+id/textLogout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:text="Log Out"
                        android:textAlignment="center"
                        android:textColor="@color/white"
                        android:textSize="16sp"
                        android:textStyle="bold" />
                </LinearLayout>
            </androidx.cardview.widget.CardView>
        </GridLayout>
    </RelativeLayout>
</RelativeLayout>
